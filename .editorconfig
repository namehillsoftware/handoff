# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
# Set default charset
[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8

[*.{java,kt,gradle}]
indent_style = tab
trim_trailing_whitespace = true
indent_size = tab
max_line_length = 120
